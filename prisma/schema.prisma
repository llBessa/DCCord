generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id         BigInt    @id @default(autoincrement())
  nome       String
  email      String
  senha      String    @db.VarChar
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}
